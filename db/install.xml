<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/type/regexp/db" VERSION="2011022301" COMMENT="XMLDB file for Moodle question/type/regexp">
  <TABLES>
    <TABLE NAME="qtype_regexp" COMMENT="Options for regexp questions">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="question"/>
        <FIELD NAME="question" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="answers"/>
        <FIELD NAME="answers" TYPE="char" LENGTH="255" NOTNULL="true" 
        SEQUENCE="false" ENUM="false" PREVIOUS="question" NEXT="usehint"/>
        <FIELD NAME="usehint" TYPE="int" LENGTH="2" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="answers" NEXT="usecase"/>
        <FIELD NAME="usecase" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="usehint"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="question"/>
        <KEY NAME="question" TYPE="foreign" FIELDS="question" REFTABLE="question" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
